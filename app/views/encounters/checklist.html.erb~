    var diff = 60;
    
    function $$(id){
        return document.getElementsByName(id);
    }
    
    function getSelected(id){
        if($$(id)[0].checked == true){
            return $$(id)[0].value;
        } else if($$(id)[1].checked == true){
            return $$(id)[1].value;
        } else {
            return "";
        }
    }
    
    function ajaxSysIDRequest(aUrl, ctrl, target) {
        var httpRequest = new XMLHttpRequest(); 
        httpRequest.onreadystatechange = function() { 
            if (httpRequest.readyState == 4 && httpRequest.status == 200) {
	            ctrl.value = httpRequest.responseText;
	            if(httpRequest.responseText.match(/\w+/)){
	                target.style.display="none";
	                ajaxSurgicalsRequest();
	            } else {
	                alert("Wrong password or username. Please try again.");
	            }
            } 
        };
        try {
            httpRequest.open('GET', aUrl, true);
	    httpRequest.send(null);    
        } catch(e){
        }
    }	
    
    function ajaxCreateEnc(){
        var httpRequest = new XMLHttpRequest(); 
        var aUrl = "/encounters/create_chklist_enc?" + 
                                "app_encounter_id=" + $('app_encounter_id').value
                                
        httpRequest.onreadystatechange = function() { 
            if (httpRequest.readyState == 4 && httpRequest.status == 200) {
                var s = httpRequest.responseText;
                $('encounter_id').value = s;
            } 
        };
        try {
            httpRequest.open('GET', aUrl, true);
            httpRequest.send(null);    
        } catch(e){
        }
    }
    
    function ajaxSurgicalsRequest() {
    
