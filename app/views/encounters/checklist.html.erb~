<% 
 
    @fields = {}
    
    if @encounter_id
    
        @obs = Observation.find(:all, :conditions => ["encounter_id = ?", @encounter_id])
    
        @obs.each{|o|
        
            case o.concept_id
                
                when 85
                                
                    @fields["tintime"] = o.value_datetime.strftime("%H:%M") rescue Time.now.strftime('%H:%M')
                
                when 86
                    
                    @fields["datetaken"] = o.value_datetime.strftime("%Y-%m-%d") rescue Time.now.strftime('%Y-%m-%d')                
                
